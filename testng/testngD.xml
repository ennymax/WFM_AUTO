        <classes>
            <class name="com.Module.ResourceAllocationManagement.CreateNewResource"/>
            <class name="com.Module.ResourceAllocationManagement.CreateResourceLevel"/>
            <class name="com.Module.ResourceAllocationManagement.UpdateResouceLevel"/>
            <class name="com.Module.ResourceAllocationManagement.CreateResourceType"/>
